@isTest
public class AddOpportunityManagerTest {
    @isTest static void testAddOpprotunityManager(){
        Account acc = new Account(Name = 'Test acc');
        INSERT acc;
        Contact con = new Contact(LastName = 'Test con', AccountId = acc.Id);
        INSERT con;
        Opportunity opp1 = new Opportunity(Name = 'Test opp1', BillToContact__c = con.Id, StageName='Prospecting',closeDate=Date.newInstance(2024, 06, 12));
        Opportunity opp2 = new Opportunity(Name = 'Test opp2', BillToContact__c = con.Id, StageName='Prospecting',closeDate=Date.newInstance(2024, 06, 12));
        List<Opportunity> oppLis = new List<Opportunity>();
        oppLis.add(opp1);
        oppLis.add(opp2);
        INSERT oppLis;
        AddOpportunityManager.addManager(oppLis);
        Opportunity updatedOpp1 = [SELECT Name, Manager__c FROM Opportunity WHERE ID = :opp1.ID];
        Opportunity updatedOpp2 = [SELECT Name, Manager__c FROM Opportunity WHERE ID = :opp2.ID];
		System.assertEquals(updatedOpp1.Manager__c, acc.Id);
        System.assertEquals(updatedOpp2.Manager__c, acc.Id);
        
    }
}