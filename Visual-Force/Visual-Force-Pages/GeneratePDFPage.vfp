<apex:page controller="GenerateStudentPDFController" renderAs="pdf" action="{!generatePDF}">
    <apex:form >
     <apex:pageBlock title="Student Details">
       <apex:pageBlockSection columns="1">
                <apex:outputText >Name: {!Student.Name}</apex:outputText>
                <apex:outputText >Class: {!Student.Class__c}</apex:outputText>
            </apex:pageBlockSection>
        </apex:pageBlock>
    </apex:form>
</apex:page>
   
